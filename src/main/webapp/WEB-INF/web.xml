<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
  	<servlet-name>DoctorServlet</servlet-name>
  	<display-name>DoctorServlet</display-name>
  	<description></description>
  	<servlet-class>org.appointment.servlets.DoctorServlet</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>AppointmentServlet</servlet-name>
  	<display-name>AppointmentServlet</display-name>
  	<description></description>
  	<servlet-class>org.appointment.servlets.AppointmentServlet</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>ViewAppointmentsServlet</servlet-name>
  	<display-name>ViewAppointmentsServlet</display-name>
  	<description></description>
  	<servlet-class>org.appointment.servlets.ViewAppointmentsServlet</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>DoctorServlet</servlet-name>
  	<url-pattern>/DoctorServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>AppointmentServlet</servlet-name>
  	<url-pattern>/AppointmentServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>ViewAppointmentsServlet</servlet-name>
  	<url-pattern>/ViewAppointmentsServlet</url-pattern>
  </servlet-mapping>
</web-app>
